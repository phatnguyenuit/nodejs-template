{
  "env": {
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "plugins": ["prettier", "jest"],
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "error"
  }
}
